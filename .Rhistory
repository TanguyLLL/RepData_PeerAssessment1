tempoDat2<- summarise(tempoDat2, mean(steps, na.rm = TRUE))
graph2<- ggplot(tempoDat2, aes(tempoDat2$`mean(steps, na.rm = TRUE)`, tempoDat2$interval))
graph2 + geom_bar(stat = "identity") + facet_grid(.~ tempoDat2$weekend)
